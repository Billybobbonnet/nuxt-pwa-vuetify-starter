{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~": "./",
          "@": "./"
        },
        "extensions": [".vue", ".js"]
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [["env", { "targets": { "node": "current" } }]],
      "plugins": [
        "transform-object-rest-spread",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
